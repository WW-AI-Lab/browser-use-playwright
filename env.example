# Browser-Use-Playwright 环境变量配置
# 复制此文件为 .env 并填入你的API密钥

# =============================================================================
# LLM 提供商配置 (至少选择其中一种)
# =============================================================================

# OpenAI 配置 (推荐)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4o
# OPENAI_BASE_URL=https://api.openai.com/v1  # 可选，自定义API端点

# Anthropic Claude 配置
# ANTHROPIC_API_KEY=your_anthropic_api_key_here
# ANTHROPIC_MODEL=claude-3-sonnet-20240229
# ANTHROPIC_BASE_URL=https://api.anthropic.com  # 可选

# DeepSeek 配置
# DEEPSEEK_API_KEY=your_deepseek_api_key_here
# DEEPSEEK_BASE_URL=https://api.deepseek.com/v1
# DEEPSEEK_MODEL=deepseek-chat

# Azure OpenAI 配置
# AZURE_OPENAI_API_KEY=your_azure_openai_api_key_here
# AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/
# AZURE_OPENAI_DEPLOYMENT=your-deployment-name
# AZURE_OPENAI_API_VERSION=2024-02-15-preview

# Google Gemini 配置
# GOOGLE_API_KEY=your_google_api_key_here
# GOOGLE_MODEL=gemini-pro

# Ollama 本地配置
# OLLAMA_BASE_URL=http://localhost:11434
# OLLAMA_MODEL=llama2

# =============================================================================
# Browser-Use 特定配置
# =============================================================================

# 浏览器配置
BROWSER_USE_HEADLESS=false
BROWSER_USE_TIMEOUT=30

# 录制配置
RECORDING_OUTPUT_DIR=./workflows/
RECORDING_AUTO_CLEANUP=true

# 执行配置
EXECUTION_BROWSER=chromium
EXECUTION_HEADLESS=false
EXECUTION_TIMEOUT=30
EXECUTION_CONCURRENT_LIMIT=10
EXECUTION_RETRY_COUNT=3

# 自愈配置
HEALING_MAX_ATTEMPTS=3
HEALING_AUTO_SAVE=true
HEALING_BACKUP_ORIGINAL=true

# =============================================================================
# 可选配置
# =============================================================================

# 日志级别 (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Chrome用户数据目录
CHROME_USER_DATA_DIR=./chrome-profiles/

# 代理配置 (可选)
# HTTP_PROXY=http://proxy.example.com:8080
# HTTPS_PROXY=https://proxy.example.com:8080

# 调试模式
# DEBUG=true 